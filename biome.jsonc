{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "extends": ["ultracite"],
  "files": {
    "includes": [
      "**",
      "!**/*.css",
      "!**/routeTree.gen.ts",
      "!**/convex/_generated"
    ]
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noConsole": "off"
      },
      "style": {
        "noHeadElement": "off"
      },
      "performance": {
        "noImgElement": "off"
      }
    }
  }
}
